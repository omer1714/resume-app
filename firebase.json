      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "resume-app-omer",
          "configurations": {
            "android": "1:479518471903:android:8828367d725570e8559b6a",
            "ios": "1:479518471903:ios:4d8779e977822510559b6a",
            "macos": "1:479518471903:ios:4d8779e977822510559b6a",
