        }
      },
      "macos": {
        "default": {
          "projectId": "resume-app-omer",
          "appId": "1:479518471903:ios:4d8779e977822510559b6a",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
